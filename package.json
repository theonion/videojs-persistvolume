{
  "name": "videojs-persistvolume",
  "version": "0.1.3",
  "description": "A plugin for Video.js that saves user's volume setting using localStorage, but falls back to cookies if necessary.",
  "main": "dist/videojs.persistvolume.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/theonion/videojs-persistvolume"
  },
  "keywords": [
    "video",
    "videojs",
    "volume"
  ],
  "author": "Sean Bloomfield <sbloomfield@theonion.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/theonion/videojs-persistvolume/issues"
  },
  "homepage": "https://github.com/theonion/videojs-persistvolume",
  "dependencies": {
    "video.js": ">=4.4.2"
  },
  "devDependencies": {
    "browserify": "^9.0.8",
    "browserify-shim": "^3.8.5",
    "gulp": "^3.8.11",
    "gulp-rename": "^1.2.2",
    "gulp-showhelp": "^1.0.2",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "^3.0.4",
    "uglify": "^0.1.3",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "browser": {
    "videojs": "video.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "videojs": "global:videojs"
  }
}
